  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Database
        <small>Customers</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Data tables</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            
            <!-- /.box-header -->
            <div class="box-body">
              <table id="pagination" class="table table-bordered table-hover">
                <thead>
                <tr>
                  <th>#</th>
                  <th>ID</th>
                  <th>NAME</th>
                  <th>AGE</th>
                  <th style="text-align: center;">ACTION</th>
                </tr>
                </thead>
                <tbody>
            <%for(var i=0;i<results1.length;i++){ %>
                <tr>
                  <td><%-results1[i].stt%></td>
                  <td><%-results1[i].id%></td>
                  <td><%-results1[i].name%></td>
                  <td><%-results1[i].age%></td>
                  <td class="ttd">
                      <a href="/update/<%-results1[i].id%>">UPDATE</a>
                      <a onclick="return confirm('Are you sure?')" href="/delete/<%-results1[i].id%>">DELETE</a>
                      <a href="/detail/<%-results1[i].id%>">DETAIL</a>
                      <!-- <a href="/update/<%-results1[i].id%>"><img style = "width: 20px; height: 20px" src="https://cdn4.iconfinder.com/data/icons/users-groups-set-2/64/x-16-2-512.png"></a>
                         <a href="/delete/<%-results1[i].id%>" onclick="return confirm('Are you sure you want to delete?');"><img style = "width: 25px; height: 25px" src="https://image.flaticon.com/icons/png/512/14/14148.png" alt=""></a>
                        <a  href="/detail/<%-results1[i].id%>"><img style = "width: 20px; height: 20px" src="data:image/png;base64," alt=""></a>  -->
                  </td>
                </tr>
                <%}%>
              </tbody>
              </table>
              <div class="row">
                <div class="col-sm-12">
                  <div class="ctn">
                    <ul class="pagination">
                      <li class="paginate_button <%-results3==1?'disabled':''%>"><a href="<%=results3==1?'#':'./'+(results3-1)%>">Previous</a></li>
                      <%for(let lap=1,j=results3>6?(results3-5):1;j<=results2;j++,lap++){%>
                      <li class="paginate_button <%-j==results3?'active':''%>"><a href="./<%=j%>"><%=j%></a></li>
                      <% if(lap>=10) break; %>
                      <%}%>
                      <li class="paginate_button <%-results3==results2?'disabled':''%>"><a href="<%=results3==results2?'#':'./'+(results3+1)%>">Next</a></li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
            <!-- /.box-body -->
            <div class="box-header">
                <h3 class="box-title"><a href="../add">Add a customer</a></h3>
              </div>
          </div>
        </div>
        <!-- /.col -->
      </div>
